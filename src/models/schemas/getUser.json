{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string"
                    },
                    "idAzure": {
                        "type": "string"
                    },
                    "uuid": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "email",
                    "role",
                    "createdAt",
                    "idAzure",
                    "uuid"
                ]
            }
        },
        "pagination": {
            "type": "object",
            "properties": {
                "total": {
                    "type": "number"
                },
                "from": {
                    "type": "number"
                },
                "to": {
                    "type": "number"
                },
                "previousPage": {
                    "type": "null"
                },
                "nextPage": {
                    "type": "null"
                }
            },
            "required": [
                "total",
                "from",
                "to",
                "previousPage",
                "nextPage"
            ]
        }
    },
    "required": [
        "data",
        "pagination"
    ]
}